{
    "save_dir": "toy_gaussian_hsweep",
    "getattr": {
        "disc_constructor": "LinearModel",
        "disc_loss_constructor": "CrossEntropyLoss",
        "disc_opt_constructor": "SGD",
        "gen_constructor": "LinearModel",
        "gen_opt_constructor": "SGD",
        "dataset_constructor": "GaussianDataset"
    },
    "disc_kwargs": {
        "n_outputs": 2
    },
    "gen_kwargs": {
        "n_outputs": 2,
        "bias": false
    },
    "trial_kwargs": {
        "n_epochs": 250,
        "disc_metric_fns": {
            "getattr": {
                "accuracy": "get_accuracy"
            }
        },
        "epoch_metric_fns": {
            "getattr": {
                "gen_oracle_loss": "get_generator_oracle_loss"
            }
        }
    },
    "train_dataset_kwargs": {
        "classes": 2,
        "useful_features": 2,
        "spurious_features": 0,
        "samples": 1000,
        "use_existing_distribution": true,
        "unit_std": true
    },
    "test_dataset_kwargs": {
        "classes": 2,
        "useful_features": 2,
        "spurious_features": 0,
        "samples": 1000,
        "use_existing_distribution": true,
        "unit_std": true
    },
    "device": "cpu",
    "params_to_tune": {
        "disc_opt_kwargs": {
            "lr": ["loguniform", [1e-5, 1e-1]],
            "momentum": ["uniform", [0.0, 0.9]]
        },
        "gen_opt_kwargs": {
            "lr": ["loguniform", [1e-5, 1e-1]],
            "momentum": ["uniform", [0.0, 0.9]]
        },
        "disc_step_kwargs": {
            "disc_steps_per_gen_step": ["loguniform", [0.1, 10.0]]
        },
        "gen_loss_constructor": ["choice", [["AdversarialMinimaxLoss", "AdversarialEntropyMaximizationLoss"]]]
    },
    "hsweep_kwargs": {
        "num_samples": 1000,
        "metric": "test_gen_oracle_loss",
        "metric_mode": "min"
    }
}