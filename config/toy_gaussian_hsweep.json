{
    "save_dir": "toy_xor_hsweep__spurious_features",
    "getattr": {
        "disc_constructor": "MlpModel",
        "disc_loss_constructor": "CrossEntropyLoss",
        "disc_opt_constructor": "SGD",
        "gen_constructor": "LinearModel",
        "gen_loss_constructor": "AdversarialEntropyMaximizationLoss",
        "gen_opt_constructor": "SGD",
        "dataset_constructor": "XorDataset"
    },
    "disc_kwargs": {
        "hidden_width": 8,
        "hidden_depth": 1,
        "n_outputs": 2
    },
    "gen_kwargs": {
        "bottleneck_width": 2,
        "bias": false,
        "sn": true
    },
    "train_dataset_kwargs": {
        "spurious_features": 8
    },
    "trial_kwargs": {
        "n_epochs": 1000,
        "disc_metric_fns": {
            "getattr": {
                "accuracy": "get_accuracy"
            }
        },
        "epoch_metric_fns": {
            "getattr": {
                "gen_oracle_loss": "get_generator_oracle_loss"
            }
        },
        "seed": 0
    },
    "device": "cpu",
    "params_to_tune": {
        "disc_opt_kwargs": {
            "lr": ["loguniform", [1e-4, 1e-2]],
            "momentum": ["uniform", [0.0, 1.0]]
        },
        "gen_opt_kwargs": {
            "lr": ["loguniform", [1e-4, 1e-2]],
            "momentum": ["uniform", [0.0, 1.0]]
        },
        "disc_step_kwargs": {
            "disc_steps_per_gen_step": ["loguniform", [0.1, 10.0]]
        }
    },
    "hsweep_kwargs": {
        "num_samples": 250,
        "metric": "test_gen_oracle_loss",
        "metric_mode": "min",
        "concurrent_trials": 4
    }
}