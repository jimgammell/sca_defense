         "trial_kwargs": {"n_epochs": [["dg", 200]], "gen_steps_per_disc_step": 5},
         "disc_loss_fn": "MSELoss",
         "gen_loss_fn": "MSELoss"},
        {"objective_formulation": "Misdirection_Max",
         "disc_loss_fn": "BCELoss",
         "gen_loss_fn": "BCELoss",
         "trial_kwargs": {"n_epochs": [["dg", 200]]}},
        {"objective_formulation": "Misdirection_Min",
         "disc_loss_fn": "BCELoss",
         "gen_loss_fn": "BCELoss",
         "trial_kwargs": {"n_epochs": [["dg", 200]]}},
        {"objective_formulation": "Randomize",
         "disc_loss_fn": "BCELoss",
