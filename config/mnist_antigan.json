{
    "trial_description": "In this trial we implement the GAN-based defense on the MNIST dataset. The goal here is to be able to visualize what the GAN defense is doing, since it is hard to visualize its effect on a power trace dataset.",
    "trial": "train_antigan",
    "disc": "DCDiscriminator",
    "gen": "DCGenerator",
    "disc_opt": "RMSprop",
    "disc_opt_kwargs": {"lr": 0.00005},
    "gen_opt": "RMSprop",
    "gen_opt_kwargs": {"lr": 0.00005},
    "latent_var_shape": [256, 100],
    "latent_var_distr": "Normal",
    "latent_var_distr_kwargs": {"loc__EXPAND": 0.0, "scale__EXPAND": 1.0},
    "use_labels": true,
    "gen_weight_clamp": null,
    "num_classes": 10,
    "dataset": "MNIST",
    "dataset_kwargs": {"transform": ["ToTensor", "Normalize"], "transform_kwargs": [{}, {"mean": [0.5], "std": [0.5]}]},
    "dataloader_kwargs": {"batch_size": 256, "shuffle": true},
    "device": "cuda",
    "trial_kwargs": {"n_epochs": [["d", 50], ["dg", 200]], "disc_steps_per_gen_step": 5},
    "expand0": [
        {"objective_formulation": "Wasserstein",
         "disc_loss_fn": "MSELoss",
         "gen_loss_fn": "MSELoss"}],
    "expand1": [
        {"disc_kwargs": {"apply_spectral_norm": true}}],
    "seed": 0
}