         "gen_loss_fn": "BCELoss"},
        {"objective_formulation": "Wasserstein",
         "disc_kwargs": {"output_transform": "Identity", "output_transform_kwargs": {}},
         "trial_kwargs": {"n_epochs": [["dg", 200]], "disc_steps_per_gen_step": 5},
         "disc_weight_clamp": 0.1,
         "disc_loss_fn": "MSELoss",
